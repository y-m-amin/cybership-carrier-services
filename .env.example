# UPS API base URL (commonly used test environment host)
UPS_BASE_URL=https://wwwcie.ups.com

# Rating API version (from UPS docs path param)
UPS_RATE_VERSION=v2409

# OAuth client credentials from UPS Developer Portal
UPS_CLIENT_ID=your_client_id
UPS_CLIENT_SECRET=your_client_secret

# OAuth token endpoint path (joined with UPS_BASE_URL)
UPS_OAUTH_TOKEN_PATH=/security/v1/oauth/token

# HTTP client timeout in milliseconds
HTTP_TIMEOUT_MS=15000

# Optional: identifies client/source app in UPS headers (defaults to "testing"/"cli" in code)
# UPS_TRANSACTION_SRC=cybership-takehome
